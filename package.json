    "vue": "^3.3.4",
    "vue-router": "4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/types": "^17.4.4",
    "@types/node": "^20.6.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "rimraf": "^5.0.1",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.2",
    "vue-tsc": "^1.8.5"
  },
  "lint-staged": {
    "*.{ts,tsx,vue,js,json}": [
      "pnpm lint:fix"
    ]
